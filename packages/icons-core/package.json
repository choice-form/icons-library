{
  "name": "@choiceform/icons-core",
  "version": "1.0.0",
  "description": "Core utilities for generating icon libraries from SVG files",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./scripts/*": "./scripts/*"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "camelcase": "^8.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.1",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.5"
  }
}
